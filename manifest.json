{
  "manifest_version": 3,
  "name": "TabKeeper - 智能标签页管理器",
  "version": "1.0.0",
  "description": "一键保存、分类管理、云同步您的标签页，支持备份防止数据丢失",
  "permissions": ["tabs", "storage", "unlimitedStorage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "保存当前窗口的所有标签页"
  },
  "chrome_url_overrides": {
    "newtab": "tabkeeper.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
